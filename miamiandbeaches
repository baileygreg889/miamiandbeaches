                        <span class="d-block">By signing up, you are agreeing to our <a href="/about-gmcvb/website-privacy-policy-cookies-and-terms">Privacy Policy</a>. We won’t share or sell your email address. You may unsubscribe at any time.</span>
                    </div>
                </div>
            <input name="__RequestVerificationToken" type="hidden" value="CfDJ8Ji_AN29_UlFiH1pO-GqfntqMFHkfnS9szFS1pF0t2tfXHYj5Q1jdkSSa370uLSqgdoqhBJCfNy6z99PuHibJD0vMqxXL7Op3okjpkks7ZRpu7Cv5TgCWIFG3MGX1RbGohvs2uqU-z4wUOLzYD9R1W8" /></form>
        </div>
    </div>
</div>
<script>

    const newsletterFormBody = document.querySelector("#newsletterFormBody"); //footer form
    const newsletterFooter = document.querySelector("#newsletterFooter"); // footer wrapper
    const newsletterFormWrapper = document.querySelector("#newsletterFormWrapper"); //footer form wrapper
    const successMessageF = document.querySelector("#newsletterFormSuccess"); // footer success area

    newsletterFormBody?.addEventListener("submit", (e) => {
        newsletterSubmitForm(e, 'footer');
    });

    const setNewsletterCookie = () => {
        const date = new Date();
        date.setMonth(date.getMonth() + 1);
        document.cookie = `newsletter_submit=false; expires=${date.toUTCString()}`;
    };

    const toggleNewsletterModalPopup = (location) => {
        if (location === 'popup') {
            newsletterPopupModal?.classList.toggle("show");
            setNewsletterCookie();
        } else if (location === 'footer') {
            newsletterFooter?.classList.toggle("show");
        }
    };

    const newsletterSubmitForm = async (e, formType) => {
        e.preventDefault();
        const data = new FormData(e.target);
        const value = Object.fromEntries(data.entries());

        try {
            const response = await fetch('/api/forms/consumer-newsletter', {
                method: 'POST',
                headers: {
                    'Content-Type': 'application/json',
                },
                body: JSON.stringify(value),
                type: 'html'
            });

            if (response.status === 200) {
                if (formType === 'popup') {
                    newsletterPopup?.classList.add("d-none");
                    successSection?.classList.remove("d-none");
                    if (successMessagePopup)
                        successMessagePopup.innerHTML = `<div class="text-center mb-5 fs-18">Your email, ${value.email}, has been subscribed to our newsletter.</div>`;
                } else if (formType === 'footer') {
                    newsletterFormWrapper?.classList.add('d-none');
                    newsletterFormBody?.classList.add('d-none');
                    successMessageF?.classList.add('d-block');
                } else {
                    inlineNewsletter?.classList.add("d-none");
                    inlineSuccessSection?.classList.remove("d-none");
                    if (inlineSuccessMessage)
                        inlineSuccessMessage.innerHTML = `<div class="text-center mb-5 fs-18">Your email, ${value.email}, has been subscribed to our newsletter.</div>`;
                }
                setNewsletterCookie();
            }
        } catch (error) {
            const errorSection = document.querySelectorAll(".general-error");
            errorSection?.forEach((section) => section.classList.add('d-block'));
        }
    };
</script>


    

    <section class="content">
        <div class="ys-newsletter mb-40 me-40" id="newsletterPopUpModal">
            <div class="ys-newsletter__container">
                <div class="container-form px-20 px-sm-30 py-15 d-flex flex-column align-items-center">
                    <div class="avatar mb-40">
                        <button class="btn close-btn" onclick="toggleNewsletterModalPopup('popup')" tabindex="0"><span class="ys-icon ys-icon-close-solid"></span></button>
                        <picture class="ys-image">
                            <img src="https://images.miamiandbeaches.com/crm/simpleview/image/upload/h_360/crm/miamifl/CityOfMiamiBeach_2B4867CD-5056-A36A-0B06BD1F65D0FBA7-2b4862e65056a36_2b486f7e-5056-a36a-0b236ac9d9cdc4fc.jpg" alt="Miami Beach" role="presentation">
                        </picture>
                    </div>
                    <div class="form d-flex flex-column">
                        <form id="newsletterPopUp" method="POST">
                            <h4>STAY IN THE KNOW WITH ALL THINGS GREATER MIAMI & MIAMI BEACH</h4>
                            <div class="ys-form-control">
                                <input aria-label="Email input" class="ys-form-control__input newsletter-input" name="email" id="email" placeholder="Email" type="email" required>
                            </div>
                            <div class="ys-form-control">
                                <input aria-label="Zip code input" class="ys-form-control__input newsletter-input" name="zipcode" id="zipcode" placeholder="Zip/Postal code" type="text" required>
                            </div>
                            <div class="d-flex align-items-center mt-4 flex-column">
                                <button type="submit" id="submitButtonPopup" aria-label="Submit button" class="ys-btn ys-btn--sm mb-20">Submit</button>
                                <span class="newsletter-disclaimer">By signing up, you are agreeing to our <a href="/about-gmcvb/website-privacy-policy-cookies-and-terms">Privacy Policy</a>. We won’t share or sell your email address. You may unsubscribe at any time.</span>
                            </div>
                        <input name="__RequestVerificationToken" type="hidden" value="CfDJ8Ji_AN29_UlFiH1pO-GqfntqMFHkfnS9szFS1pF0t2tfXHYj5Q1jdkSSa370uLSqgdoqhBJCfNy6z99PuHibJD0vMqxXL7Op3okjpkks7ZRpu7Cv5TgCWIFG3MGX1RbGohvs2uqU-z4wUOLzYD9R1W8" /></form>
                        <div class="d-flex d-none align-items-center flex-column" id="successSection">
                            <h4 class="fs-24 mb-3 text-center">THANK YOU!</h4>
                            <label id="successMessage" class="ys-form-check__label"></label>
                            <button type="button" class="ys-btn ys-btn--sm mb-5" onclick="toggleNewsletterModalPopup('popup')">CLOSE</button>
                            <span class="newsletter-disclaimer">By signing up, you are agreeing to our <a href="/about-gmcvb/website-privacy-policy-cookies-and-terms">Privacy Policy</a>. We won’t share or sell your email address. You may unsubscribe at any time.</span>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <script>
        /* Items Specific to Popup */
        /* Remaining funcitons are in NewsletterSignupPartial */

        const newsletterPopupModal = document.querySelector("#newsletterPopUpModal"); // popup modal
        const newsletterPopup = document.querySelector("#newsletterPopUp"); //popup form
        const successMessagePopup = document.querySelector("#successMessage"); // pupup success message
        const successSectionPopup = document.querySelector("#successSection"); //popup success area
        var collapseTimeout = '';

        function cancelTimeout() {
            clearTimeout(collapseTimeout);
        }

        collapseTimeout = setTimeout(() => {
            newsletterPopupModal.classList.toggle("show");
        }, 4000);

        newsletterPopupModal?.addEventListener("focus", cancelTimeout);

        newsletterPopup?.addEventListener("submit", (e) => {
            newsletterSubmitForm(e, 'popup');
        });

    </script>



    
